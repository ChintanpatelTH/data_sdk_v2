{
  "plugins": [
    "git-tag",
    "conventional-commits",
    ["upload-assets", { "assets": ["dist"] }]
  ],
  "owner": "ChintanpatelTH",
  "repo": "data_sdk_v2",
  "name": "Chintan Patel",
  "email": "chintan.patel@techholding.co",
  "onlyPublishWithReleaseLabel": true,
  "baseBranch": "main",
